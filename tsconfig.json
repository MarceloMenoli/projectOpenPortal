{
    "compilerOptions": {
      "target": "ES2021",
      "module": "ESNext",
      "moduleResolution": "Node",
      "jsx": "react",
      "sourceMap": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true
    }
  }
  